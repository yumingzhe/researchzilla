<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.Subject" table="researchzilla_subject">
        <id name="id" column="id" type="int">
            <generator class="increment"/>
        </id>
        <property name="subjecttopic" column="subjecttopic" type="string" length="50" not-null="true"/>
        <property name="researchdirection" column="researchdirection" type="string" length="100" not-null="true"/>
        <property name="researchmember" column="researchmember" type="string" length="100"  />
        <property name="starttime" column="starttime" type="string"  />
        <property name="prograss" column="prograss" type="string"  />
    </class>
</hibernate-mapping>